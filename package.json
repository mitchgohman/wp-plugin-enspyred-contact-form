{
    "name": "enspyred-contact-form",
    "private": true,
    "version": "1.1.1",
    "type": "module",
    "scripts": {
        "watch": "vite build --watch",
        "build": "vite build",
        "clean": "rm -rf build",
        "prebuild": "npm run clean",
        "sync-version": "node sync-version.js",
        "prepackage": "npm run sync-version",
        "package": "npm run build && npm run package:create",
        "package:create": "rm -rf dist/enspyred-contact-form && mkdir -p dist/enspyred-contact-form && rsync -av --exclude-from=.distignore . dist/enspyred-contact-form/ && find dist/enspyred-contact-form -name '.DS_Store' -delete"
    },
    "dependencies": {
        "axios": "^1.9.0",
        "i18n-iso-countries": "^7.14.0",
        "prop-types": "^15.8.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-google-recaptcha-v3": "^1.10.1",
        "react-phone-number-input": "^3.4.12",
        "react-select": "^5.10.1",
        "styled-components": "^6.1.17"
    },
    "devDependencies": {
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "globals": "^16.0.0",
        "vite": "^6.3.1"
    },
    "author": "Mitch Gohman",
    "license": "ISC",
    "browserslist": [
        "defaults",
        "not IE 11",
        "maintained node versions"
    ]
}
